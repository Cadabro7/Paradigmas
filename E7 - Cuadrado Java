public class Cuadrado{
    private String color;
    private double lado;
    
    public Circulo(String color, double lado){
        this.color=color;
        this.lado=lado;
    }
    private double area(){
        return lado * lado;
    }
    private double perimetro(){
        return 2 * (4 * lado);
    }
    public double getArea(){
        return area();
    }
    public double getPerimetro(){
        return perimetro();
    }
    public double getColor(){
        return color;
    }
}
